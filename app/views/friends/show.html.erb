  	   <% @friend_to_user.each do |friend| %>
				<div class="col-sm-offset-1  col-sm-3">
					<div class="panel-success">
						<img src='<%= friend.image.url %>' class="img-circle" alt="Cinque Terre" width="170" height="150">
							<div class="textDesc" align="center">
								<h4><code><%= friend.username %></code></h4>
							    <%= link_to 'Unfriend', 
							    	friends_path(:param1 => current_user, :param2 => friend.id),
               						      method: :delete,
               						      data: { confirm: 'Are you sure you want to delete friend?' } %>
						    </div>
						 </img>
				   </div>
			</div>
  		<%end%>
 